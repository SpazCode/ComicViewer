{
  "name": "Comic Viewer", 
  "version": "1.1.0.0",
  "manifest_version": 2,
	"offline_enabled": true,
	"icons": {
        "16": "img/16.png",
        "48": "img/48.png",
        "128": "img/128.png"
    }, 
    "app": {
        "background" : {
            "scripts": ["js/background.js"]
        }
    },
	"permissions": [
    "fullscreen",
    "storage",
    {"fileSystem": ["write", "retainEntries"]}
  ]
}