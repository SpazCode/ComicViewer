{
  "name": "Comic Viewer", 
  "version": "2.0.0.1",
  "manifest_version": 3,
    "offline_enabled": true,
    "icons": {
        "16": "img/16.png",
        "48": "img/48.png",
        "128": "img/128.png"
    }, 
    "app": {
        "background" : {
            "scripts": ["js/background.js"]
        }
    },
    "permissions": [
    "fullscreen",
    "storage",
    {"fileSystem": ["write", "retainEntries"]}
  ]
}